<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="dx168.com.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.VideoActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity.CalendarActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity.RecyclerViewTestActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity.DataBaseActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity.PlRefreshRecyclerViewActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity.MultipleRecyclerViewActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity.SwipeRefreshRecyclerViewActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>